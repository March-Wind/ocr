import cv2

# 定义整个区域的左右边界
left_boundary = 66.0
right_boundary = 469.0

# 定义小矩形列表（左上角x, 左上角y, 右下角x, 右下角y）
rectangles = [(66, 79, 66, 143), (130, 79, 136, 143), (160, 79, 160, 143)]

# 加载图片
img_path = "your_image_path.jpg"
image = cv2.imread(img_path)

# 遍历小矩形，计算左右区域的长度并判断哪个更长
max_length = 0
max_length_image = None

for rect in rectangles:
    left_rect_x1, _, right_rect_x2, _ = rect

    # 计算左侧区域的长度
    left_region_length = left_rect_x1 - left_boundary

    # 计算右侧区域的长度
    right_region_length = right_boundary - right_rect_x2

    # 判断左右区域哪个更长
    if left_region_length >= right_region_length:

        # 裁剪左侧区域的图片
        cropped_image = image[:, int(left_boundary) : int(left_rect_x1)]
    elif right_region_length > left_region_length:

        # 裁剪右侧区域的图片
        cropped_image = image[:, int(right_rect_x2) : int(right_boundary)]


# 显示裁剪出的最长区域图片
cv2.imshow("Max Length Region", max_length_image)
cv2.waitKey(0)
cv2.destroyAllWindows()

